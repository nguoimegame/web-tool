---
import HashPage from '../../../components/HashPage.astro';
import { languages } from '../../../i18n/ui';
import { useTranslations } from '../../../i18n/utils';

export function getStaticPaths() {
  return Object.keys(languages).map((lang) => {
    return { params: { lang } };
  });
}

const { lang = 'en' } = Astro.params;
const t = useTranslations(lang as any);
---

<HashPage
  title={t('keccak_384.title')}
  description={t('keccak_384.description')}
  algorithmName="keccak_384"
  jsFile="https://cdnjs.cloudflare.com/ajax/libs/js-sha3/0.9.3/sha3.min.js"
  activePage="hash/keccak_384"
/>
