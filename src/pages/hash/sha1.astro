---
import HashPage from '../../components/HashPage.astro';
import { useTranslations } from '../../i18n/utils';

const t = useTranslations('en');
const title = t('sha1.title');
const description = t('sha1.description');
const algorithmName = "sha1";
const jsFile = "https://cdnjs.cloudflare.com/ajax/libs/js-sha1/0.7.0/sha1.min.js";
const activePage = "hash/sha1";
---

<HashPage 
  title={title}
  description={description}
  algorithmName={algorithmName}
  jsFile={jsFile}
  activePage={activePage}
/>
