---
import HashPage from '../../components/HashPage.astro';
import { useTranslations } from '../../i18n/utils';

const t = useTranslations('en');
---

<HashPage 
  title={t('sha512_224.title')}
  description={t('sha512_224.description')}
  algorithmName="sha512_224"
  jsFile="https://cdnjs.cloudflare.com/ajax/libs/js-sha512/0.9.0/sha512.min.js"
  activePage="hash/sha512_224"
  methodWrapper="window.sha512.sha512_224"
/>
