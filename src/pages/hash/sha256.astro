---
import HashPage from '../../components/HashPage.astro';
import { useTranslations } from '../../i18n/utils';

const t = useTranslations('en');
const title = t('sha256.title');
const description = t('sha256.description');
const algorithmName = "sha256";
const jsFile = "https://cdnjs.cloudflare.com/ajax/libs/js-sha256/0.11.0/sha256.min.js";
const activePage = "hash/sha256";
---

<HashPage 
  title={title}
  description={description}
  algorithmName={algorithmName}
  jsFile={jsFile}
  activePage={activePage}
/>
