---
import HashPage from '../../components/HashPage.astro';
import { useTranslations } from '../../i18n/utils';

const t = useTranslations('en');
const title = t('sha512.title');
const description = t('sha512.description');
const algorithmName = "sha512";
const jsFile = "https://cdnjs.cloudflare.com/ajax/libs/js-sha512/0.9.0/sha512.min.js";
const activePage = "hash/sha512";
---

<HashPage 
  title={title}
  description={description}
  algorithmName={algorithmName}
  jsFile={jsFile}
  activePage={activePage}
/>
