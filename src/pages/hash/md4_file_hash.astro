---
import FileHashPage from '../../components/FileHashPage.astro';
import { useTranslations } from '../../i18n/utils';

const t = useTranslations('en');
const title = t('md4_file_hash.title');
const description = t('md4_file_hash.description');
const algorithmName = "MD4";
const jsFile = "md4.min.js";
const activePage = "hash/md4_file_hash";
---
<FileHashPage 
    title={title}
    description={description}
    algorithmName={algorithmName}
    jsFile={jsFile}
    activePage={activePage}
/>
