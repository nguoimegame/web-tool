---
import FileHashPage from '../../components/FileHashPage.astro';
import { useTranslations } from '../../i18n/utils';

const t = useTranslations('en');
const title = t('sha256_checksum.title');
const description = t('sha256_checksum.description');
const algorithmName = "SHA256";
const jsFile = "sha256.min.js";
const activePage = "hash/sha256_checksum";
---
<FileHashPage 
    title={title}
    description={description}
    algorithmName={algorithmName}
    jsFile={jsFile}
    activePage={activePage}
/>
