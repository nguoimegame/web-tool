---
import EncodingPage from '../../../components/EncodingPage.astro';

const title = "Constant Case Converter";
const description = "Convert string to constant case. (Screaming snake case, MACRO_CASE)";
const toolName = "Convert";
const jsFile = "change-case.min.js";
const initScript = "window.method = ChangeCase.constantCase; methodLoad();";
const activePage = "case/constant/";
const reverseLink = "";
---

<EncodingPage 
    title={title}
    description={description}
    toolName={toolName}
    jsFile={jsFile}
    initScript={initScript}
    activePage={activePage}
    reverseLink={reverseLink}
    hasInputSettings={false}
    hasOutputSettings={false}
    hasFormatSettings={false}
/>
