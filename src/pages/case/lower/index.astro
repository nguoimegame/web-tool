---
import EncodingPage from '../../../components/EncodingPage.astro';

const title = "Lower Case Converter";
const description = "Convert string to lower case. (Downcase)";
const toolName = "Convert";
const initScript = "window.method = function toLowerCase(s) { return s.toLowerCase(); }; methodLoad();";
const activePage = "case/lower/";
const reverseLink = "";
---

<EncodingPage 
    title={title}
    description={description}
    toolName={toolName}
    initScript={initScript}
    activePage={activePage}
    reverseLink={reverseLink}
    hasInputSettings={false}
    hasOutputSettings={false}
    hasFormatSettings={false}
/>
