---
import EncodingPage from '../../../components/EncodingPage.astro';

const title = "Upper Case Converter";
const description = "Convert string to upper case. (Upcase, All caps)";
const toolName = "Convert";
const initScript = "window.method = function toUpperCase(s) { return s.toUpperCase(); }; methodLoad();";
const activePage = "case/upper/";
const reverseLink = "";
---

<EncodingPage 
    title={title}
    description={description}
    toolName={toolName}
    initScript={initScript}
    activePage={activePage}
    reverseLink={reverseLink}
    hasInputSettings={false}
    hasOutputSettings={false}
    hasFormatSettings={false}
/>
