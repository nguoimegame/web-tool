---
import EncodingPage from '../../../components/EncodingPage.astro';

const title = "Upper Camel Case Converter";
const description = "Convert string to upper camel case (Pascal case).";
const toolName = "Convert";
const jsFile = "change-case.min.js";
const initScript = "window.method = ChangeCase.pascalCase; methodLoad();";
const activePage = "case/upper-camel/";
const reverseLink = "";
---

<EncodingPage 
    title={title}
    description={description}
    toolName={toolName}
    jsFile={jsFile}
    initScript={initScript}
    activePage={activePage}
    reverseLink={reverseLink}
    hasInputSettings={false}
    hasOutputSettings={false}
    hasFormatSettings={false}
/>
