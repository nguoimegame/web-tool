---
import EncodingPage from '../../components/EncodingPage.astro';
const title = "File to Base32";
const description = "Encode file to Base32. Free online Base32 encoding tool.";
const toolName = "FileEncode";
const jsFile = "base32.js?v=3";
const activePage = "encoding/base32_encode_file";
const reverseLink = "encoding/base32_decode_file";
---
<EncodingPage 
    title={title}
    description={description}
    toolName={toolName}
    jsFile={jsFile}
    activePage={activePage}
    reverseLink={reverseLink}
    hasInputSettings={false}
    hasOutputSettings={false}
    hasFormatSettings={false}
/>
