---
import EncodingPage from '../../components/EncodingPage.astro';
const title = "Hex Encode";
const description = "Encode text or file to Hex. Free online Hex encoding tool.";
const toolName = "Encode";
const jsFile = "hex.js?v=3";
const activePage = "encoding/hex_encode";
const reverseLink = "encoding/hex_decode";
---
<EncodingPage 
    title={title}
    description={description}
    toolName={toolName}
    jsFile={jsFile}
    activePage={activePage}
    reverseLink={reverseLink}
    hasInputSettings={true}
    hasOutputSettings={false}
    hasFormatSettings={false}
/>
