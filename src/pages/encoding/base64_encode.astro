---
import EncodingPage from '../../components/EncodingPage.astro';
const title = "Base64 Encode";
const description = "Encode text or file to Base64. Free online Base64 encoding tool.";
const toolName = "Encode";
const jsFile = "base64.js?v=3";
const activePage = "encoding/base64_encode";
const reverseLink = "encoding/base64_decode";
---
<EncodingPage 
    title={title}
    description={description}
    toolName={toolName}
    jsFile={jsFile}
    activePage={activePage}
    reverseLink={reverseLink}
    hasInputSettings={true}
    hasOutputSettings={false}
    hasFormatSettings={true}
/>
