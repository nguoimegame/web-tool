---
import EncodingPage from '../../components/EncodingPage.astro';
const title = "Base32 Encode";
const description = "Encode text or file to Base32. Free online Base32 encoding tool.";
const toolName = "Encode";
const jsFile = "base32.js?v=3";
const activePage = "encoding/base32_encode";
const reverseLink = "encoding/base32_decode";
---
<EncodingPage 
    title={title}
    description={description}
    toolName={toolName}
    jsFile={jsFile}
    activePage={activePage}
    reverseLink={reverseLink}
    hasInputSettings={true}
    hasOutputSettings={false}
    hasFormatSettings={false}
/>
