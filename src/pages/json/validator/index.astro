---
import EncodingPage from '../../../components/EncodingPage.astro';

const title = "JSON Validator";
const description = "Free online JSON validateter. Format, minify, validate or view JSON data.";
const toolName = "Validate";
const jsFile = "json.js?v=6";
const activePage = "json/validator/";
const reverseLink = "";
---

